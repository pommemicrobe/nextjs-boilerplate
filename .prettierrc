{
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-prisma", "@trivago/prettier-plugin-sort-imports"],
  "semi": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "endOfLine": "auto",
  "trailingComma": "all",
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
